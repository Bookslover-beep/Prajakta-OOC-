#include <iostream>
using namespace std;

struct student
{
	string name;
	int roll_no;
	char div;
	float marks;
};
int main()
{
	
	cout << "Name  : Prajakta Arun Magar\n";
    cout << "Roll No: 14\n";
    cout << "Class : S.Y.CSE-B\n";
   
	student s;
	cout<<"Enter Name:";
	cin>>s.name;
	cout<<"Enter Roll_no:";
	cin>>s.roll_no;
	cout<<"Enter Div:";
	cin>>s.div;
	cout<<"Enter Marks:";
	cin>>s.marks;
	
	
	cout<<"Name:" <<s.name <<endl;
	cout<<"Roll_no:" <<s.roll_no <<endl;
	cout<<"Div:" <<s.div <<endl;
	cout<<"Marks:" <<s.marks <<endl;
	
	return 0;
	
}
